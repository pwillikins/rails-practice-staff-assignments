<%= link_to "New Person", new_person_path, class: "pull-right" %>
<h1>Staff</h1>

<table class="table">
  <thead>
  <tr>
    <th>Name</th>
  </tr>
  </thead>
  <tbody>
  <% @people.each do |person| %>
    <tr>
      <td>
         <a href="people/<%= person.id %>"><%= person.first_name %> <%= person.last_name %></a>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
